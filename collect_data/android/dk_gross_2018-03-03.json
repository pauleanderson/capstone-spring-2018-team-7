  { title: 'Plants vs. Zombies‚Ñ¢ Heroes',
    summary: 'A heroic new collectible card game from the makers of Plants vs. Zombies 2!',
    icon: '//lh3.googleusercontent.com/3LwyZ3NidtGEixfOyqBP0HZDuF1J2WBdzDixxxYgk7sNl3FIGaveVjzaQidzBOtzEA=w300',
    price: '0',
    free: true,
    minInstalls: 10000000,
    maxInstalls: 50000000,
