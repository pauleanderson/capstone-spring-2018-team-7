  { title: 'Super Mario Run',
    summary: 'Remix 10 is here!',
    icon: '//lh3.googleusercontent.com/6geoqgXLoeCK4-A1xxkR9KcGcI_KF23SGeHX1sAM2gauaIhNp515feFNS3ML1zmCugI=w300',
