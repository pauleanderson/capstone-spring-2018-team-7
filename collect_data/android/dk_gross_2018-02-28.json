    score: 4.5,
    reviews: 92812,
    developer: 'Big Fish Games',
    developerId: '8355317828905497231',
    developerEmail: 'android.email@bigfishgames.com',
    developerWebsite: 'https://www.bigfishgames.com/daily/fairway-solitaire/',
    developerAddress: '333 Elliott Ave W.\nSuite 200\nSeattle, Wa 98119',
    updated: 'January 29, 2018',
