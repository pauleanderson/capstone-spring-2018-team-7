    score: 4.1,
    reviews: 229271,
    developer: 'Snail Games USA Inc',
    developerId: 'Snail+Games+USA+Inc',
    developerEmail: 'support@snailgamesusa.com',
    developerWebsite: 'http://panda.snail.com',
    developerAddress: '1424 Lincoln Blvd, Santa Monica, CA 90401, USA',
