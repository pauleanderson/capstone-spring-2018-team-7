    developerEmail: 'ad-myshelf-egsupport@nttsolmare.com',
    developerWebsite: 'http://myshelfapp.com/',
    developerAddress: 'No.2 Sumitomo Bldg. 4-7-28 Kitahama Chuo-Ku, Osaka 541-0041, Japan',
