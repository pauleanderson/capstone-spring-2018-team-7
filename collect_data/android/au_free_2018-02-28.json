    score: 4.5,
    reviews: 883656,
    developer: 'Toca Boca',
    developerId: '8567646723268777320',
    developerEmail: 'support@tocaboca.com',
    developerWebsite: 'http://www.tocaboca.com',
    developerAddress: 'Lumaparksv√§gen 9\n120 31 Stockholm\nSweden',
    updated: 'June 29, 2017',
    version: '1.2.3-play',
