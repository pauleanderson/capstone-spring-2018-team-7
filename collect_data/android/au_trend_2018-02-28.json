    developer: 'Clever Mind Games',
    developerId: 'Clever+Mind+Games',
    developerEmail: 'clevermindgames@gmail.com',
    developerWebsite: 'http://quadropoly.com.au',
    developerAddress: 'PO Box 115 CLARINDA VIC 3169',
    updated: 'February 25, 2018',
