    developer: 'Kick Square Studio',
    developerId: 'Kick+Square+Studio',
    developerEmail: 'kicksquarestudio@gmail.com',
    developerWebsite: undefined,
    developerAddress: 'The Arcade,\nMahadevapura Road,\nBangalore 560048',
