    summary: 'Grand Theft Auto: San Andreas',
    icon: '//lh3.googleusercontent.com/XRlCpj22PFJuFK43QcE3u5RxmoNUed7YO_zG-6F1PCqjrBIIURbTBDvTgLVVIk5pNeY=w300',
