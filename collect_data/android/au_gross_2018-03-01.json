    score: 4.3,
    reviews: 490038,
    developer: '2K, Inc.',
    developerId: '6681606924556273560',
    developerEmail: 'wwesupercardsupport@2k.com',
    developerWebsite: 'http://support.2k.com/',
    developerAddress: '9 Hamilton Landing\nNovato, CA. 94949',
