    developerEmail: 'support@dragonsoul.zendesk.com',
    developerWebsite: 'http://dragonsoulgame.com',
    developerAddress: '6-10-1 Roppongi\nRoppongi Hills Mori Tower\nMinato-ku\nTokyo\nJapan',
