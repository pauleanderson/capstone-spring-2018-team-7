    reviews: 1094685,
    developer: 'Ubisoft Entertainment',
    developerId: '4647998716943242763',
    developerEmail: 'android.support@ubisoft.com',
    developerWebsite: 'https://www.ubisoft.com/en-gb/game/hungry-shark-world/',
