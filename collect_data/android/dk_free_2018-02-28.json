  { title: 'Clash Royale',
    summary: 'Clash Royale is a real-time, head-to-head battle game set in the Clash Universe.',
    icon: '//lh3.googleusercontent.com/K-MNjDiO2WwRNwJqPZu8Wd5eOmFEjLYkEEgjZlv35hTiua_VylRPb04Lig3YZXLERvI=w300',
