    developerEmail: 'espen@snowcannongames.com',
    developerWebsite: 'http://www.snowcannongames.com/',
    developerAddress: '2600 El Camino Real, Suite 601\nPalo Alto CA 94306\nUSA',
    updated: 'April 27, 2017',
