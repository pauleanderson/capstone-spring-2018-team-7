    developer: 'NEXON M Inc.',
    developerId: '6133955275329314641',
    developerEmail: 'support@nexonm.com',
    developerWebsite: 'https://www.nexonm.com/support/',
    developerAddress: '6121 Hollis Street\nEmeryville CA 94608\nUnited States',
